{
  "name": "@ui5/migration",
  "version": "0.2.0",
  "description": "Tooling to support the migration of UI5 projects by adapting code for new UI5 framework versions",
  "author": "SAP SE (https://www.sap.com)",
  "license": "Apache-2.0",
  "keywords": [
    "openui5",
    "sapui5",
    "ui5",
    "migration",
    "tool"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.5"
  },
  "scripts": {
    "build": "rimraf js && tsc -p .",
    "build-watch": "rimraf js && tsc -w -p .",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "npm run lint && npm run check && npm run coverage",
    "test-azure": "npm run lint && npm run check && npm run unit && npm run coverage-junit",
    "coverage": "nyc npm run unit",
    "coverage-junit": "nyc --reporter=text --reporter=text-summary --reporter=cobertura npm run unit-junit",
    "unit": "mocha ./js/test/*Test.js ./js/test/**/*Test.js ./js/test/**/*/**Test.js --timeout 6000",
    "unit-junit": "npm run unit -- --reporter mocha-junit-reporter",
    "unit-watch": "npm run unit -- -w",
    "check": "gts check",
    "clean": "gts clean",
    "fix": "gts fix",
    "preversion": "npm run build && npm test",
    "version": "git-chglog --next-tag v$npm_package_version -o CHANGELOG.md && git add CHANGELOG.md",
    "prepublishOnly": "git push --follow-tags",
    "release-note": "git-chglog -c .chglog/release-config.yml v$npm_package_version"
  },
  "types": "typings/index.d.ts",
  "files": [
    "index.js",
    "CONTRIBUTING.md",
    "npm-shrinkwrap.json",
    "bin/**",
    "defaultConfig/**",
    "js/src/**/*.js"
  ],
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "js/**/*.d.ts",
      "docs/**",
      "jsdocs/**",
      "coverage/**",
      "test/**",
      "bin/**",
      "lib/lbt/**",
      "util/**",
      "src/cli.ts",
      "index.js",
      "js/src/cli.js"
    ],
    "check-coverage": true,
    "lines": 50,
    "statements": 50,
    "functions": 50,
    "branches": 40,
    "watermarks": {
      "lines": [
        60,
        90
      ],
      "functions": [
        60,
        90
      ],
      "branches": [
        50,
        70
      ],
      "statements": [
        60,
        90
      ]
    },
    "cache": true,
    "all": true,
    "report-dir": "./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:SAP/ui5-migration.git"
  },
  "dependencies": {
    "diff": "^4.0.2",
    "esprima": "^4.0.1",
    "globals": "^12.4.0",
    "graceful-fs": "^4.2.4",
    "ignore": "^5.1.8",
    "json5": "^2.1.3",
    "minimatch": "^3.0.4",
    "recast": "^0.18.10",
    "request": "^2.88.2",
    "semver": "^7.3.2",
    "yargs": "^15.3.1"
  },
  "bin": {
    "ui5-migration": "./bin/cli.js"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/esprima": "^4.0.2",
    "@types/estree": "0.0.41",
    "@types/graceful-fs": "^4.1.2",
    "@types/json5": "^0.0.30",
    "@types/minimatch": "^3.0.3",
    "@types/mocha": "^5.2.6",
    "@types/node": "^13.13.12",
    "@types/yargs": "^15.0.5",
    "gts": "^1.1.2",
    "mocha": "^6.2.3",
    "mocha-junit-reporter": "^1.23.3",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^8.1.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "typescript": "^3.9.5"
  }
}
